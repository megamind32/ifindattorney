{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(self)"
        },
        {
          "key": "Feature-Policy",
          "value": "geolocation 'self'"
        }
      ]
    }
  ]
}
